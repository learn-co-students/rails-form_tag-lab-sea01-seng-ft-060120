<h3> "Student Form" </h3>

<%= form_tag students_path do %>
    <label>Enter First Name</label><br>
    <%= text_field_tag :'student[first_name]' %><br>
    <label>Enter Last Name</label><br>
    <%= text_field_tag :'student[last_name]' %><br>

    <%= submit_tag :'Submit Student' %>

 <% end %>

 <%= params.inspect %>